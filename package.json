{
  "name": "@symbaloo/elm-css-modules-loader",
  "version": "1.0.2",
  "description": "webpack loader for using CSS modules in Elm code",
  "main": "index.js",
  "repository": "git@github.com:symbaloo/elm-css-modules-loader.git",
  "publishConfig": {
    "registry": "https://nexus.symbaloo.tw/repository/npm-internal"
  },
  "keywords": [
    "webpack",
    "loader",
    "elm",
    "css"
  ],
  "author": "Kevin Yank <kevin@cultureamp.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/cultureamp/elm-css-modules-loader/issues"
  },
  "homepage": "https://github.com/cultureamp/elm-css-modules-loader",
  "peerDependencies": {
    "webpack": ">=1.14.0 <5"
  },
  "files": [
    "index.js"
  ],
  "release": {
    "preset": "angular",
    "parserOpts": {
      "noteKeywords": [
        "BREAKING CHANGE",
        "BREAKING CHANGES",
        "BREAKING",
        "ðŸ’”"
      ]
    },
    "verifyConditions": [
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/npm",
      "./ci/prepareElmRelease.js",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "elm.json"
        ]
      }
    ],
    "publish": [
      "@semantic-release/npm",
      "./ci/publishElmRelease.js",
      "@semantic-release/github"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "loader-utils": "^1.1.0"
  }
}
